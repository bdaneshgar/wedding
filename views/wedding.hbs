    <style>
      .hero {
        position: relative;
        overflow: hidden;
      }
      .hero-overlay {
        position: absolute;
        top: 0;
        left: 0;
        width: 100%;
        height: 100%;
        background: rgba(0, 0, 0, 0.4);
        z-index: 1;
      }
      .hero-content {
        position: relative;
        z-index: 2;
      }
      .hero-content img {
        width: min(420px, 80vw);
        margin: 0 auto;
        display: block;
      }
      .hero .container {
        min-height: 90vh;
        display: flex;
        flex-direction: column;
        justify-content: flex-start;
        align-items: center;
        padding-top: 40px;
        padding-bottom: 40px;
      }
      .confetti-canvas {
        position: fixed;
        inset: 0;
        width: 100%;
        height: 100%;
        pointer-events: none;
        z-index: 4000;
      }
      .intro-image {
        opacity: 0;
        transition: transform 0.85s ease-out, opacity 0.85s ease-out;
        will-change: transform, opacity;
      }
      .intro-image-left {
        transform: translate3d(-200px, 0, 0);
      }
      .intro-image-right {
        transform: translate3d(200px, 0, 0);
      }
      .intro-image-active {
        transform: translate3d(0, 0, 0);
        opacity: 1;
      }
      .intro-story {
        display: grid;
        grid-template-columns: minmax(140px, 18vw) minmax(0, 1fr) minmax(140px, 18vw);
        gap: clamp(24px, 5vw, 48px);
        align-items: end;
        justify-content: center;
        max-width: 960px;
        margin: clamp(32px, 6vw, 64px) auto 0;
        padding: clamp(24px, 4vw, 48px);
      }
      .intro-story__text {
        max-width: min(72ch, 88vw);
        margin: 0 auto;
        text-align: center;
      }
      .intro-story__text p {
        margin-bottom: 0;
      }
      .intro-story__img {
        justify-self: center;
        align-self: end;
        width: min(100%, clamp(180px, 10vw, 240px));
        height: auto;
      }
      [data-intro-trigger] {
        display: block;
        width: 100%;
        height: 1px;
        margin-top: 0;
        pointer-events: none;
        opacity: 0;
      }
      /* Explicit background color rules for sections after #rsvp and others */
      #intro {
        background-color: #f9f9f9;
      }
      #map {
        background-color: #ffffff;
      }
      #rsvp {
        background-color: #f9f9f9;
      }
      footer {
        background-color: #ffffff;
      }
      .invitation-card {
        border: none;
        border-radius: 18px;
        box-shadow: 0 24px 60px rgba(0, 0, 0, 0.1);
        background: rgba(255, 255, 255, 0.94);
        font-family: 'Libre Baskerville', serif;
      }
      .invitation-card .card-body {
        padding: 48px 40px;
      }
      .invitation-logo {
        width: min(280px, 70%);
        margin: 0 auto 24px;
        display: block;
      }
      .invitation-names {
        font-family: 'Playfair Display', serif;
        font-weight: 600;
        letter-spacing: 0.3em;
        font-size: clamp(2.4rem, 6vw, 3.4rem);
      }
      .invitation-ampersand {
        display: inline-flex;
        align-items: center;
        justify-content: center;
        gap: 28px;
        margin: 18px 0 22px;
        color: rgba(66, 78, 92, 0.7);
        font-family: 'Libre Baskerville', serif;
        font-size: 1.3rem;
        letter-spacing: 0.2em;
      }
      .invitation-ampersand::before,
