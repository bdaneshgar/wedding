<section id="eng-pics" class="section-padding">
    <div class="container-fluid">
        <div class="row">
            <div class="col-md-12 text-center">
                <h3 class="header">Photo Gallery</h3>
            </div>
        </div>
        {{#if galleryPhotos.length}}
        <div class="good-times-grid">
            {{#each galleryPhotos}}
            <div class="good-times-item">
                <a class="fancybox" rel="good-times" href="{{this.href}}">
                    <div class="img-wrap">
                        <div class="overlay"><i class="fa fa-search"></i></div>
                        <img
                          data-lazy-image
                          data-src="{{this.src}}"
                          {{#if this.srcset}}data-srcset="{{this.srcset}}"{{/if}}
                          alt="{{this.alt}}"
                          loading="lazy"
                          decoding="async">
                        <noscript>
                          <img src="{{this.src}}" alt="{{this.alt}}">
                        </noscript>
                    </div>
                </a>
            </div>
            {{/each}}
        </div>
        {{else}}
        <div class="row">
            <div class="col-md-6 col-md-offset-3 text-center">
                <p>No photos yet—add images to <code>public/img/good_times</code> to populate the gallery.</p>
            </div>
        </div>
        {{/if}}
    </div>
    <div class="container">
        <div class="row hidden-lg hidden-md hidden-xs">
            <div class="col-xs-12 section-border section-padding"></div>
        </div>
    </div>
</section>

