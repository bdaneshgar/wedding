<script>
  function initWeddingMap() {
    var mapCanvas = document.getElementById('map-canvas');
    if (!mapCanvas || typeof google === 'undefined' || !google.maps) {
      console.warn('[invitation] map canvas unavailable');
      return;
    }

    var bhHotel = new google.maps.LatLng(34.0810, -118.4143);
    var map = new google.maps.Map(mapCanvas, {
      zoom: 16,
      center: bhHotel,
      mapTypeId: google.maps.MapTypeId.ROADMAP,
      streetViewControl: false,
      fullscreenControl: false,
      mapTypeControl: false,
      zoomControl: true
    });

    new google.maps.Marker({
      position: bhHotel,
      map: map,
      title: 'Beverly Hills Hotel',
      animation: google.maps.Animation.DROP
    });

    google.maps.event.addDomListener(window, 'resize', function () {
      map.setCenter(bhHotel);
    });
  }
</script>
