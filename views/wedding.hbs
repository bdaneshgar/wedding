      .section-subtitle {
        max-width: 660px;
        margin: 0 auto 2.25em;
        color: rgba(0, 0, 0, 0.6);
        font-size: 1.05em;
      }
      .schedule-card {
        border: none;
        border-radius: 20px;
        box-shadow: 0 30px 60px rgba(0, 0, 0, 0.08);
        background: rgba(255, 255, 255, 0.95);
