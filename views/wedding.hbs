<script>
  window.addEventListener('DOMContentLoaded', function () {
    var introSection = document.getElementById('intro');
    if (!introSection) {
      return;
    }

    var introImages = introSection.querySelectorAll('[data-intro-image]');
    if (!introImages.length) {
      return;
    }

    var activateSlides = function () {
      introImages.forEach(function (img) {
        img.classList.add('intro-image-active');
      });
    };

    if (!('IntersectionObserver' in window)) {
      setTimeout(activateSlides, 1000);
      return;
    }

    var triggered = false;
    var observer = new IntersectionObserver(function (entries) {
      entries.forEach(function (entry) {
        if (entry.isIntersecting && !triggered) {
          triggered = true;
          setTimeout(activateSlides, 1000);
          observer.disconnect();
        }
      });
    }, { threshold: 0.45 });

    observer.observe(introSection);
  });
</script>
<script>
  window.addEventListener('load', function () {
    var CDN_URL = 'https://cdn.jsdelivr.net/npm/canvas-confetti@1.9.3/dist/confetti.browser.min.js';
    var CDN_SCRIPT_ID = 'confetti-cdn-script';
    var POLL_DELAY = 140;
    var MAX_ATTEMPTS = 60;
    var hasLaunched = false;
    var attempts = 0;
    var confettiCanvas = null;
    var launcher = null;
    var animationInterval = null;
    var cleanupTimeout = null;
    var pollTimer = null;
    var prefersReducedMotion = window.matchMedia && window.matchMedia('(prefers-reduced-motion: reduce)').matches;

    function ensureCanvas() {
      if (confettiCanvas && confettiCanvas.parentNode) {
        return confettiCanvas;
      }
      var canvas = document.createElement('canvas');
      canvas.className = 'confetti-canvas';
      canvas.style.position = 'fixed';
      canvas.style.left = '0';
      canvas.style.top = '0';
      canvas.style.width = '100%';
      canvas.style.height = '100%';
      canvas.style.pointerEvents = 'none';
      canvas.style.zIndex = '4000';
      document.body.appendChild(canvas);
      confettiCanvas = canvas;
      return canvas;
    }

    function teardownCanvas() {
      if (confettiCanvas && confettiCanvas.parentNode) {
        confettiCanvas.parentNode.removeChild(confettiCanvas);
      }
      confettiCanvas = null;
    }

    function cleanup() {
      if (animationInterval) {
        window.clearInterval(animationInterval);
        animationInterval = null;
      }
      if (cleanupTimeout) {
        window.clearTimeout(cleanupTimeout);
        cleanupTimeout = null;
      }
      if (launcher && typeof launcher.reset === 'function') {
        try {
          launcher.reset();
        } catch (err) {
          // ignore
        }
      }
      launcher = null;
      teardownCanvas();
      hasLaunched = false;
    }

    function ensureLauncher() {
      if (launcher) {
        return launcher;
      }
      if (typeof confetti !== 'function') {
        return null;
      }
      var canvas = ensureCanvas();
      try {
        launcher = confetti.create(canvas, { resize: true, useWorker: true });
      } catch (err) {
        launcher = null;
      }
      if (!launcher) {
        try {
          launcher = confetti.create(canvas, { resize: true, useWorker: false });
        } catch (err) {
          launcher = null;
        }
      }
      if (!launcher) {
        teardownCanvas();
      }
      return launcher;
    }

    function fireConfetti() {
      if (prefersReducedMotion) {
        return true;
      }
      if (hasLaunched) {
        return true;
      }

      var activeLauncher = ensureLauncher();
      if (!activeLauncher) {
        return false;
      }

      hasLaunched = true;

      var defaults = {
        spread: 95,
        startVelocity: 64,
        ticks: 560,
        gravity: 0.82,
        decay: 0.9,
        scalar: 1.12,
        angle: 275
      };
      var launchColumns = [0.18, 0.5, 0.82];
      var shots = 0;
      var maxShots = 18;

      animationInterval = window.setInterval(function () {
        launchColumns.forEach(function (baseX) {
          var jitter = (Math.random() - 0.5) * 0.18;
          var originX = Math.min(Math.max(baseX + jitter, 0.05), 0.95);
          activeLauncher(Object.assign({}, defaults, {
            particleCount: 150,
            origin: { x: originX, y: 0.9 }
          }));
        });
        shots += 1;
        if (shots >= maxShots) {
          window.clearInterval(animationInterval);
          animationInterval = null;
        }
      }, 160);

      cleanupTimeout = window.setTimeout(cleanup, (maxShots * 160) + 6000);
      return true;
    }

    function requestConfetti() {
      if (typeof confetti === 'function') {
        return fireConfetti();
      }

      if (!document.getElementById(CDN_SCRIPT_ID)) {
        var script = document.createElement('script');
        script.id = CDN_SCRIPT_ID;
        script.src = CDN_URL;
        script.async = true;
        script.onload = fireConfetti;
        script.onerror = function () {
          if (pollTimer) {
            window.clearInterval(pollTimer);
            pollTimer = null;
          }
        };
        document.head.appendChild(script);
      }
      return false;
    }

    if (!requestConfetti()) {
      pollTimer = window.setInterval(function () {
        attempts += 1;
        if (requestConfetti() || attempts >= MAX_ATTEMPTS) {
          window.clearInterval(pollTimer);
          pollTimer = null;
          if (!hasLaunched) {
            cleanup();
          }
        }
      }, POLL_DELAY);
    }
  });
</script>
