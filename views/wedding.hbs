<script>
  window.addEventListener('DOMContentLoaded', function () {
    var introSection = document.getElementById('intro');
    if (!introSection) {
      return;
    }

    var introImages = Array.prototype.slice.call(introSection.querySelectorAll('[data-intro-image]'));
    if (!introImages.length) {
      return;
    }

    var sentinel = introSection.querySelector('[data-intro-trigger]');
    if (!sentinel) {
      sentinel = document.createElement('span');
      sentinel.setAttribute('data-intro-trigger', '');
      sentinel.setAttribute('aria-hidden', 'true');
      sentinel.style.display = 'block';
      sentinel.style.width = '100%';
      sentinel.style.height = '1px';
      sentinel.style.pointerEvents = 'none';
      sentinel.style.opacity = '0';
      introSection.appendChild(sentinel);
    }

    var activated = false;

    var activateSlides = function () {
      if (activated) {
        return;
      }
      activated = true;
      introImages.forEach(function (img, index) {
        window.setTimeout(function () {
          img.classList.add('intro-image-active');
        }, index * 80);
      });
    };

    var fallbackTimer = window.setTimeout(activateSlides, 6000);

    if ('IntersectionObserver' in window) {
      var observer = new IntersectionObserver(function (entries) {
        entries.forEach(function (entry) {
          if (entry.isIntersecting) {
            observer.disconnect();
            window.clearTimeout(fallbackTimer);
            window.setTimeout(activateSlides, 120);
          }
        });
      }, { threshold: 0, rootMargin: '-50% 0px 0px 0px' });

      observer.observe(sentinel);
    } else {
      var onScroll = function () {
        if (activated) {
          return;
        }
        var rect = sentinel.getBoundingClientRect();
        var viewportHeight = window.innerHeight || document.documentElement.clientHeight || 0;
        if (rect.top <= viewportHeight) {
          window.removeEventListener('scroll', onScroll);
          window.removeEventListener('resize', onScroll);
          window.clearTimeout(fallbackTimer);
          window.setTimeout(activateSlides, 120);
        }
      };

      window.addEventListener('scroll', onScroll, { passive: true });
      window.addEventListener('resize', onScroll);
      onScroll();
    }
  });
</script>
<script>
  window.addEventListener('load', function () {
    var lazyImages = Array.prototype.slice.call(document.querySelectorAll('[data-lazy-image]'));
    if (!lazyImages.length) {
      return;
    }

    var hydrate = function (img) {
      if (img.dataset.srcset) {
        img.srcset = img.dataset.srcset;
        img.removeAttribute('data-srcset');
      }
      if (img.dataset.src) {
        img.src = img.dataset.src;
        img.removeAttribute('data-src');
      }
      img.removeAttribute('data-lazy-image');
    };

    if ('IntersectionObserver' in window) {
      var observer = new IntersectionObserver(function (entries) {
        entries.forEach(function (entry) {
          if (!entry.isIntersecting) {
            return;
          }
          observer.unobserve(entry.target);
          hydrate(entry.target);
        });
      }, { rootMargin: '200px 0px', threshold: 0.05 });

      lazyImages.forEach(function (img) {
        observer.observe(img);
      });
    } else {
      lazyImages.forEach(hydrate);
    }
  });
</script>
<script>
  window.addEventListener('load', function () {
    if (window.matchMedia && window.matchMedia('(prefers-reduced-motion: reduce)').matches) {
      return;
    }

    function launchConfetti() {
      var settings = {
        particleCount: 180,
        origin: { y: 0.75 },
        gravity: 0.6,
        ticks: 320
      };

      confetti(Object.assign({}, settings, { spread: 70, scalar: 1.05 }));
      confetti(Object.assign({}, settings, { spread: 110, decay: 0.92, scalar: 0.9 }));
    }

    if (typeof confetti === 'function') {
      launchConfetti();
    } else {
      var tries = 0;
      var maxTries = 20;
      var interval = window.setInterval(function () {
        tries += 1;
        if (typeof confetti === 'function') {
          window.clearInterval(interval);
          launchConfetti();
        } else if (tries >= maxTries) {
          window.clearInterval(interval);
        }
      }, 150);
    }
  });
</script>
